{
  "name": "material-dashboard-pro-react",
  "version": "1.10.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "axios": "^0.21.1",
    "chartist": "0.11.4",
    "classnames": "2.3.1",
    "match-sorter": "6.3.0",
    "moment": "2.29.1",
    "node-sass": "6.0.1",
    "nouislider": "15.2.0",
    "perfect-scrollbar": "1.5.2",
    "react": "17.0.2",
    "react-big-calendar": "0.33.6",
    "react-bootstrap-sweetalert": "5.2.0",
    "react-chartist": "0.14.4",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.4",
    "react-image-gallery": "^1.2.7",
    "react-jvectormap": "0.0.16",
    "react-responsive-carousel": "^3.2.20",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-swipeable-views": "0.14.0",
    "react-table": "7.7.0",
    "react-tagsinput": "3.19.0",
    "styled-components": "^5.3.0"
  },
  "scripts": {
    "start-dev": "sh -ac '. .env.dev; react-scripts start'",
    "start-prod": "sh -ac '. .env.prod; react-scripts start'",
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "local-build": "sh -ac '. .env.prod; react-scripts build && gulp licenses'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.css",
    "minify-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.css --source-map true"
  },
  "optionalDependencies": {
    "@babel/core": "7.14.8",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  }
}
